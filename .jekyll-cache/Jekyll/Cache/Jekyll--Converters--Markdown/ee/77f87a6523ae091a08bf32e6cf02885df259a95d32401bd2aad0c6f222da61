    <span class="p">],</span>
    <span class="na">location</span><span class="p">:</span> <span class="p">{</span>
      <span class="na">city</span><span class="p">:</span> <span class="dl">'</span><span class="s1">San Francisco</span><span class="dl">'</span><span class="p">,</span>
      <span class="na">state</span><span class="p">:</span> <span class="dl">'</span><span class="s1">CA</span><span class="dl">'</span><span class="p">,</span>
      <span class="na">country</span><span class="p">:</span> <span class="dl">'</span><span class="s1">USA</span><span class="dl">'</span>
    <span class="p">}</span>
  <span class="p">}</span>
<span class="p">};</span>

<span class="kd">function</span> <span class="nx">addFriend</span><span class="p">(</span><span class="nx">userObj</span><span class="p">,</span> <span class="nx">friend</span><span class="p">)</span> <span class="p">{</span>
  <span class="c1">// change code below this line  </span>
  <span class="nx">userObj</span><span class="p">.</span><span class="nx">data</span><span class="p">.</span><span class="nx">friends</span><span class="p">.</span><span class="nx">push</span><span class="p">(</span><span class="nx">friend</span><span class="p">)</span>
  <span class="k">return</span> <span class="nx">userObj</span><span class="p">.</span><span class="nx">data</span><span class="p">.</span><span class="nx">friends</span>
  <span class="c1">// change code above this line</span>
<span class="p">}</span>

<span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">addFriend</span><span class="p">(</span><span class="nx">user</span><span class="p">,</span> <span class="dl">'</span><span class="s1">Pete</span><span class="dl">'</span><span class="p">));</span>

