<span class="nx">foods</span><span class="p">[</span><span class="dl">"</span><span class="s2">grapes</span><span class="dl">"</span><span class="p">]</span> <span class="o">=</span> <span class="mi">35</span>
<span class="nx">foods</span><span class="p">[</span><span class="dl">"</span><span class="s2">strawberries</span><span class="dl">"</span><span class="p">]</span> <span class="o">=</span> <span class="mi">27</span>
<span class="c1">// change code above this line</span>

<span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">foods</span><span class="p">);</span>
</code></pre></div></div>

